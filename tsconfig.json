{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "app/",
    "paths": {
      "@/*": ["*"],
      "@/app/*": ["app/*"],
      "@/animations/*": ["animations/*"],
      "@/components/theme/*": ["components/theme/*"],
      "@/components/*": ["components/*"],
      "@/public/*": ["public/*"],
      "@/images/*": ["images/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "resources/**/*.ts", "resources/**/*.tsx"],
  "exclude": ["node_modules"]
}
